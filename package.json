{
  "name": "lookup-server",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.17.2",
    "coveralls": "^2.13.1",
    "express": "4.13.3",
    "fs": "0.0.2",
    "webpack": "^2.5.1"
  },
  "scripts": {
    "start": "nodemon --exec concurrently \"npm run server\" \"npm run client\"",
    "server": "babel-node --presets es2015,stage-2 server/server.js",
    "install:client": "npm run babel-node install-client.js",
    "client": "babel-node start-client.js",
    "test:client": "babel-node start-client-test.js",
    "dev": "echo \"This command has been deprecated. Use 'npm start'\" && exit 1",
    "test": "npm run test:client",
    "lint": "eslint ."
  },
  "devDependencies": {
    "concurrently": "3.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "nodemon": "^1.11.0"
  }
}
